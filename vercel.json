{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "signin.html", "use": "@vercel/static" },
    { "src": "signup.html", "use": "@vercel/static" },
    { "src": "home.html", "use": "@vercel/static" },
    { "src": "jazz.html", "use": "@vercel/static" },
    { "src": "database.js", "use": "@vercel/node" },
    { "src": "database.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/signin", "dest": "/home.html" },
    { "src": "/signin", "dest": "/signin.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/jazz", "dest": "/jazz.html" },
    { "src": "/api/(.*)", "dest": "/database.js" },
    { "src": "/database", "dest": "/database.html" }
  ]
}
